<%- layout('layouts/main') %>

  <div class="row">
    <div class="col-md-4 offset-md-4 align-items-lg-center">
      <div class="card mt-4 text-center">
        <div class="card-header">
          <img
            src="https://th.bing.com/th/id/R.661ea9454f28a544a70d842854110f0f?rik=18F%2bW9lxMw5%2fNA&riu=http%3a%2f%2ftese.edu.mx%2fimagenes2004%2f3107_OCPMHSV.jpg&ehk=0WV9B506g1T5%2f9rydau83Q%2fw3wz%2brDiK7M0bYRJctDc%3d&risl=&pid=ImgRaw&r=0"
            class="rounded mx-auto d-block" alt="" width="90" height="74">


          <h3> Iniciar sesion</h3>

        </div>
        <div class="card-body">
          <form action="/signin" method="POST">
            <div class="form-group">
              <input type="email" name="email" placeholder="Ingresa un correo" class="form-control">
            </div>
            <div class="form group">
              <input type="password" name="password" placeholder="Ingresa un Password" class="form-control">
            </div>

            <button type="submit" class="btn btn-primary btn-block mt-4 ">
              Entrar
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>